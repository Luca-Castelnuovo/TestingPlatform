# custom error pages
#ErrorDocument 404 /error

# turn mod_rewrite on
RewriteEngine On

# remove .php extensions from files


# block access to .txt
<Files ~ "\.txt$">
    Order allow,deny
    Deny from all
</Files>

# allow access to robots.txt
<FilesMatch robots\.txt>
    Allow from all
</FilesMatch>

RewriteRule ^test/test$ test.php?get=shirt [QSA]